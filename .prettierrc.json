{
  "useTabs": false,
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "es5",
  "jsxBracketSameLine": false,
  "semi": true,
  "importOrder": [
    "(next.*)",
    "(.*react.*)",
    "<THIRD_PARTY_MODULES",
    "^@/components/layout$",
    "^@/components/global$",
    "^@/components/pages$",
    "^@/components/screens$",
    "^@/components/ui$",
    "^@/types$",
    "^@/data$",
    "^@/utils$",
    "^@/context$",
    "^(.*.package.json)$",
    "^(./App)$",
    "^(./styles/*)",
    "^(.*.module.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}